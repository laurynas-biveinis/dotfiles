[core]
        excludesfile = ~/.gitignore
	pager = diff-so-fancy | less --tabs=4 -RFX
[magit]
	hideCampaign = true
[diff]
	colorMoved = default
	renamelimit = 100000
	external = difft
[fetch]
	prune = true
	jobs = 0
[rerere]
	enabled = false
[commit]
	gpgsign = true
[submodule]
	fetchJobs = 0
[pull]
	ff = only
[merge]
	conflictstyle = zdiff3
[interactive]
	diffFilter = diff-so-fancy --patch
[color]
	ui = true
